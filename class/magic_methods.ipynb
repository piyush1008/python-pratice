    "class Person:\n",
    "    def __init__(self, name, age):\n",
    "        self.name=name\n",
    "        self.age=age\n",
    "    ##overriding the __str__ magic methods\n",
    "    def __str__(self):\n",
    "        return f\"{self.name} , {self.age}\"\n",
    "\n",
    "p=Person(\"piyush\",12)\n",
    "print(p)"
   ]
